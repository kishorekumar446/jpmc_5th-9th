}
root@main-tf:~# cat git.tf
terraform {
  required_providers {
    github = {
      source = "integrations/github"
      version = "6.0.0-rc1"
    }
  }
}

provider "github" {
  token = "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"
}

resource "github_repository" "repo" {
  name        = "mytest-repo"
  description = "My awesome codebase"
  visibility = "public"
}
