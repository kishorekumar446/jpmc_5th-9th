root@main-tf:~# cat first.tf
provider "aws" {
  region     = "ap-south-1"
#  access_key = "my-access-key"
#  secret_key = "my-secret-key"
}


resource "aws_instance" "ec2" {
  ami                     = "ami-0a7cf821b91bcccbc" #ubuntu
  instance_type           = "t2.micro"
  availability_zone= "ap-south-1b"
  tags = {
    Name = "raman-test"
  }
}






